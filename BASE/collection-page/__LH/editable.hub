in product.variants -%}\\n                                                    <option {%  if variant.id ==  variant_selected.id %} selected=\\\"selected\\\" {%  endif  %} data-sku=\\\"{{ variant.sku }}\\\" value=\\\"{{ variant.id }}\\\">{{ variant.title }} - {{ variant.price | money_with_currency }}<\\\/option>\\n                                                {%- endfor -%}\\n                                            <\\\/select>\\n                                        <\\\/div>\\n                                        <div class=\\\"lh-product-form__actions\\\">\\n                                            <button type=\\\"submit\\\" class=\\\"lh-quick-shop-submit\\\">@lang(section.settings.trans_add_cart)<\\\/div>\\n                                        <\\\/div>\\n                                    <\\\/div>\\n                                {%  endif %}\\n                               \\n                               \\n                                 <\\\/div>\\n                                <div class=\\\"lh-product-information\\\">\\n                                    <h3 class=\\\"lh-product-title\\\">\\n                                        <a href=\\\"{{ product.url }}\\\" title=\\\"{{ product.title | escape }}\\\">{{ product.title }}<\\\/a>\\n                                    <\\\/h3>\\n                                    @if(section.settings.show_rating == 'yes')\\n \\t\\t\\t\\t\\t\\t\\t\\t\\t\\t<div class=\\\"lh-rating-wrapper\\\">\\n \\t\\t\\t\\t\\t\\t\\t\\t\\t\\t\\t{% render 'layouthub_review',  review_type: 'rating' ,product: product %}\\n \\t\\t\\t\\t\\t\\t\\t\\t\\t\\t<\\\/div>\\n \\t\\t\\t\\t\\t\\t\\t\\t\\t@endif\\n                                    <div class=\\\"lh-product-prices\\\">\\n                                        <ins>{{ product.price  | money }}<\\\/ins>\\n                                        {% if product.price_varies %}\\n                                            <del>{{ product.compare_at_price_max | mon